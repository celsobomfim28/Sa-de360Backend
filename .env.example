# Environment
NODE_ENV=development

# Server
PORT=3000
API_VERSION=v1

# Database
DATABASE_URL="postgresql://postgres:16051986@localhost:5432/saude360?schema=public"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# Redis (Cache)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Security
BCRYPT_SALT_ROUNDS=12
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=http://localhost:5173,http://localhost:5174

# Logging
LOG_LEVEL=debug

# Notifications Scheduler
NOTIFICATIONS_AUTO_RUN_ENABLED=true
NOTIFICATIONS_INTERVAL_MS=900000
NOTIFICATIONS_STARTUP_DELAY_MS=15000